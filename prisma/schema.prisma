generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider     = "mysql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

model User {
  id        Int    @id @default(autoincrement())
  uuid      String @default(uuid())
  full_name String
  gender    String
  email     String
  password  String
}
