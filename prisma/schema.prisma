generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider     = "mysql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

model User {
  uuid         String    @id @default(uuid())
  full_name    String
  gender       String
  email        String
  password     String
  phone_number String?
  birth_day    DateTime?
  age          Int?
  location     String?
  occupation   String?
  created_date DateTime  @default(now())
  next_update  DateTime?
}
